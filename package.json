{
  "name": "watcode",
  "version": "1.0.0",
  "scripts": {
    "dev": "webpack-dev-server --config config/webpack/dev.js --inline --hot",
    "dev:server": "webpack --config config/webpack/dev.js && node dist/server.js",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "tdd": "jest --watchAll",
    "test": "jest",
    "storybook": "start-storybook -p 3002 -c config/storybook",
    "build-storybook": "build-storybook",
    "mobile": "react-native start"
  },
  "license": "MIT",
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/**/*.{js,jsx}",
      "!**/src/components/*.js",
      "!**/src/*.js",
      "!**/src/store/*.js",
      "!**/node_modules/**",
      "!**/*.stories.{js,jsx}"
    ],
    "coverageDirectory": "<rootDir>/coverage",
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "setupFiles": [
      "<rootDir>/config/jest/setup.js"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "^\\.\\.\\/\\.\\.$": "<rootDir>/config/jest/componentsMock.js",
      "^.*containers$": "<rootDir>/config/jest/componentsMock.js"
    }
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "express": "^4.16.3",
    "history": "^4.7.2",
    "humps": "^2.0.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.5",
    "moment": "^2.22.1",
    "normalizr": "^3.2.4",
    "prop-types": "^15.6.1",
    "query-string": "^6.0.0",
    "react": "16.3.1",
    "react-dom": "^16.3.1",
    "react-helmet": "^5.2.0",
    "react-native": "^0.55.3",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-native": "^4.2.0",
    "react-router-redux": "^5.0.0-alpha.9",
    "recompose": "^0.27.0",
    "redux": "^3.7.2",
    "redux-form": "^7.3.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "serialize-javascript": "^1.4.0",
    "styled-components": "^3.2.6",
    "styled-theme": "^0.3.3",
    "universal-cookie": "^2.1.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^4.0.0-alpha.3",
    "@storybook/addon-console": "^1.0.0",
    "@storybook/addon-links": "^4.0.0-alpha.3",
    "@storybook/addon-notes": "^4.0.0-alpha.3",
    "@storybook/addon-viewport": "^4.0.0-alpha.3",
    "@storybook/addons": "^4.0.0-alpha.3",
    "@storybook/react": "^4.0.0-alpha.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-native": "4.0.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "jest-cli": "^22.4.3",
    "node-sass": "^4.8.3",
    "react-test-renderer": "^16.3.2",
    "sass-loader": "^6.0.7",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.20.3",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.1"
  }
}
